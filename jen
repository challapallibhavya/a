pipeline
{
   agent any
   stages
 {
      stage('Check validate'){
      steps{
          echo 'validating the program'
       }
   }
      stage('Compilation'){
      steps{
          echo 'Compiling the program'
       }
    }
       stage('Installation'){
      steps{
          echo 'Installaing the program'
       }
     }
     stage('Deploy'){
       steps{
           echo 'deploying the program'
         } 
      }
  }
   
  }
